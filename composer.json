{
	"name" : "coka/pagination-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka pagination bundle",
	"homepage" : "https://github.com/CedrickOka/pagination-bundle",
	"license" : "MIT",
	"keywords" : [
		"twig",
		"doctrine",
		"pagination"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.2",
		"doctrine/annotations" : "^1.2",
		"symfony/dependency-injection" : "4.4.*",
		"symfony/config" : "4.4.*",
		"symfony/translation" : "4.4.*",
		"symfony/validator" : "4.4.*"
	},
	"require-dev" : {
		"doctrine/doctrine-bundle": "^2.0",
		"doctrine/mongodb-odm-bundle": "^4.0",
		"doctrine/orm": "^2.4.8",
		"symfony/framework-bundle": "4.4.*",
		"symfony/phpunit-bridge": "4.4.*",
		"symfony/yaml": "^5.0",
		"twig/twig": "^2.5"
	},
	"suggest" : {
		"doctrine/doctrine-bundle" : "~1.4",
		"doctrine/mongodb-odm-bundle" : "^4.0",
		"doctrine/orm" : "^2.4.8",
		"twig/twig" : "^2.5"
	},
	"config" : {
		"sort-packages" : true
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "4.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/pagination-bundle/blob/master/src/Resources/doc/index.md"
	}
}
