{
	"name" : "coka/pagination-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka pagination bundle",
	"homepage" : "https://github.com/CedrickOka/pagination-bundle",
	"license" : "MIT",
	"keywords" : [
		"twig",
		"doctrine",
		"pagination"
	],
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\" : "src/"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"doctrine/annotations" : "^1.2",
		"symfony/framework-bundle" : "^3.4 || ^4.0",
		"symfony/event-dispatcher" : "^3.4 || ^4.0",
		"symfony/dependency-injection" : "^3.4 || ^4.0",
		"symfony/console" : "^3.4 || ^4.0",
		"symfony/config" : "^3.4 || ^4.0",
		"symfony/translation" : "^3.4 || ^4.0",
		"symfony/validator" : "^3.4 || ^4.0"
	},
	"require-dev" : {
		"alcaeus/mongo-php-adapter" : "^1.1",
		"doctrine/doctrine-bundle" : "~1.4",
		"doctrine/orm" : "^2.4.8",
		"doctrine/mongodb-odm-bundle" : "^3.4",
		"symfony/phpunit-bridge" : "^3.0",
		"twig/twig" : "^2.5"
	},
	"suggest" : {
		"alcaeus/mongo-php-adapter" : "^1.1",
		"doctrine/doctrine-bundle" : "~1.4",
		"doctrine/mongodb-odm-bundle" : "^3.4",
		"doctrine/orm" : "^2.4.8",
		"twig/twig" : "^2.5"
	},
	"config" : {
		"platform" : {
			"ext-mongo" : "1.6.16"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "3.x.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/pagination-bundle/blob/master/src/Resources/doc/index.md"
	}
}
