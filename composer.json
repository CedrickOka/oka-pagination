{
	"name" : "oka/pagination-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony Oka pagination bundle",
	"keywords" : [
		"doctrine2",
		"pagination"
	],
	"licence" : "MIT",
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "cedrickoka@fermentuse.com"
		}
	],
	"version" : "1.0.0-dev",
	"minimum-stability" : "dev",
	"require" : {
		"php" : ">=5.5.0",
		"symfony/symfony" : "2.3.*",
		"symfony/http-foundation" : "2.6.*@dev",
		"doctrine/orm" : ">=2.2",
		"doctrine/common" : ">=2.2",
		"twig/extensions" : "1.0.*"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\Tests\\" : "Tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev"
		}
	}
}