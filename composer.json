{
	"name" : "coka/pagination-bundle",
	"description" : "Symfony Oka pagination bundle",
	"type" : "symfony-bundle",
	"keywords" : [
		"doctrine2",
		"pagination"
	],
	"licence" : "MIT",
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "cedrickoka@fermentuse.com"
		}
	],
	"version" : "1.0.0-stable",
	"minimum-stability" : "dev",
	"require" : {
		"php" : ">=5.3.9",
		"symfony/symfony" : "2.8.*",
		"doctrine/orm" : "^2.4.8",
		"doctrine/doctrine-bundle" : "~1.4",
		"doctrine/mongodb-odm" : "~1.0",
		"doctrine/mongodb-odm-bundle" : "~3.0",
		"twig/extensions" : "1.0.*"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\PaginationBundle\\Tests\\" : "Tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev"
		}
	}
}